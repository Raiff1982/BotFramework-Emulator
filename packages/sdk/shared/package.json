{
  "name": "@bfemulator/sdk-shared",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && npm run build:prod",
    "build:prod": "babel ./src --out-dir build --extensions \".ts,.tsx\" --ignore \"**/*.spec.ts\"",
    "clean": "rimraf ./built",
    "lint": "eslint --color --quiet --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-object-rest-spread": "^7.24.5",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "24.0.13",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^29.7.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-notice": "^0.7.7",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^29.3.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "5.0.0",
    "rimraf": "^2.6.2",
    "typescript": "^4.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.5",
    "botframework-config": "^4.4.0",
    "botframework-schema": "^4.17.0",
    "eslint-plugin-security": "^1.4.0",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2"
  },
  "jest": {
    "setupFilesAfterEnv": ["../../../../testSetup.js"],
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "babel-jest",
      "^.+\\.(ts?|js?)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": ["node"],
      "url": "http://localhost"
    },
    "rootDir": "./src",
    "testMatch": [
      "**/?(*.)(spec|test).(ts)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
